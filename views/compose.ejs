
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Details</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
        <label>Price</label>
        <textarea class="form-control" name="postPrice" rows="5" cols="30"></textarea>
        <label>Time</label>
        <textarea class="form-control" name="postTime" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
